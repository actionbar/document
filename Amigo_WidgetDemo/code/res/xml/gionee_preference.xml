            amigo:amigotitle="EditTextPreferenceTitle" />
    </amigoui.preference.AmigoPreferenceButtonCategory>

    <amigoui.preference.AmigoPreferenceButtonCategory
        amigo:amigokey="test2"
        amigo:amigoCategoryBtnText="测试"
        amigo:amigotitle="Test2" >
        <amigoui.preference.AmigoSwitchPreference
            amigo:amigosummary="switchpreference"
            amigo:amigotitle="@string/app_name" />

        <amigoui.preference.AmigoListPreference
            amigo:amigodefaultValue="Phone"
            amigo:amigodefaultResult="Phone"
            amigo:amigodialogTitle="dialog title"
            amigo:amigoentries="@array/pref_sms_save_location_choices"
            amigo:amigoentryValues="@array/pref_sms_save_location_values"
            amigo:amigokey="pref_key_sms_save_location2"
            amigo:amigosummary="listpreferencePhonePhonePhonePhonePhonePhonePhoneee"
            amigo:amigotitle="title" />
    </amigoui.preference.AmigoPreferenceButtonCategory>
    <amigoui.preference.AmigoPreferenceCategory
        amigo:amigokey="test3"
        amigo:amigotitle="Test3" >
        <amigoui.preference.AmigoSwitchPreference
            amigo:amigosummary="switchpreference"
            amigo:amigotitle="@string/app_name" />

        <amigoui.preference.AmigoListPreference
            amigo:amigodefaultValue="Phone"
            amigo:amigodefaultResult="Phone"
            amigo:amigodialogTitle="dialog title"
            amigo:amigoentries="@array/pref_sms_save_location_choices"
            amigo:amigoentryValues="@array/pref_sms_save_location_values"
            amigo:amigokey="pref_key_sms_save_location"
            amigo:amigosummary="listpreference"
            amigo:amigotitle="title" />
    </amigoui.preference.AmigoPreferenceCategory>

    <amigoui.preference.AmigoCheckBoxAndClickPreference
        amigo:amigokey="pref_key_check_click"
        amigo:amigosummary="checkbox and click"
        amigo:amigotitle="@string/app_name" />
    
    <amigoui.preference.AmigoPreference
        amigo:amigokey="pref_key_version"
        amigo:amigosummary="amigoFrameworkVersion"
        amigo:amigotitle="amigoFrameworkVersion" />
    
    <amigoui.preference.AmigoPreference
        amigo:amigokey="pref_key_ua"
        amigo:amigosummary="pref_key_ua"
        amigo:amigoShowPreferenceArrow="true"
        amigo:amigotitle="amigoFrameworkUA" />
    
    <amigoui.preference.AmigoPreference
        amigo:amigokey="pref_key_ua_cxt"
