<?xml version="1.0" encoding="UTF-8"?>
<amigoui.preference.AmigoPreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:amigo="http://schemas.android.com/apk/res/com.amigo.widgetdemol" >

    <amigoui.preference.AmigoPreferenceButtonCategory
        amigo:amigokey="test1"
        amigo:amigoCategoryBtnText="测试"
        amigo:amigotitle="Test1" >
        <amigoui.preference.AmigoPreference
            amigo:amigosummary="preferencepreferencepreferencepreferencepreferencepreferencepreferencepreference"
            amigo:amigotitle="@string/app_name" />

        <com.demo.preference.GnCornerPreference
            amigo:amigosummary="GnCornerPreference"
            amigo:amigotitle="GnCornerPreference" />

        <amigoui.preference.AmigoCheckBoxPreference
            amigo:amigoicon="@drawable/item_head_icon"
            amigo:amigosummary="TestSummaryTooLongDisplayEffectTestSummaryTooLongDisplayEffect2"
            amigo:amigotitle="TestTitleTooLongDisplayEffectTestTitleTooLongDisplayEffect2" />

        <amigoui.preference.AmigoEditTextPreference
